{
    "version": 2,
    "name": "gp-mock",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/companies",
            "methods": [
                "POST",
                "GET"
            ],
            "dest": "src/index.ts"
        }
    ]
}